SonarQube 업무별 위반 항목 조회 쿼리

select 
com.project_uuid --AXj3QsNyx2lmwCIzO7io
, com.created_at
, com.kee --ssg_DF_SY:str/src/main/java/com/ssgdfs/erp/sy/web/SYAP020_AController.java
, com.name --SYAP020_AController.java
, com.scope --FIL
, com.language --java
, com.long_name --str/src/main/java/com/ssgdfs/erp/sy/web/SYAP020_AController.java
, rul.plugin_name --pmd
, rul.rule_type
, case 
       when rul.rule_type in (0, 1)  then 'Code Smell' 
       when rul.rule_type = 2 then 'Bug' 
       when rul.rule_type = 3 then 'Vulnerability' 
              end 
, iss.severity --INFO
, rul.name --Unused imports
, iss.message --Avoid unused imports such as 'java.util.Locale'
, iss.line --4
, iss.status --OPEN
from issues iss, projects com, rules rul
where 
iss.component_uuid = com.uuid
and iss.rule_id = rul.id
and com.kee like 'ssg_CP_MBS_batch:%'
and iss.status = 'OPEN'
--and com."name" = 'ModelUtils.java'
--and rul.rule_type in (3)
--and iss.message = 'Make the enclosing instance method ''static'' or remove this set on the ''static'' field.'
--and rul."name" = 'Instance members should not write to "static" fields'
--and iss.project_uuid = 'AXj3QsNyx2lmwCIzO7io' --면세점팀
--and iss.component_uuid = 'AXj3QsuSL187J7p8WUtB' -- str/.../com/ssgdfs/erp/sy/service/impl/SYAP010ServiceImpl.java
--and iss.message = 'Avoid empty if statements' -- 


