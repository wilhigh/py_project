view = function(key,base_dpt_id) { 
    var auth = false;
    
    if('#{session.user.dpt_id}' == base_dpt_id) {
        auth = true; 
    }
    
    showWin('/xefc/jsp/ui/popup_layout.jsp','uri=/xefc/jsp/ui/form/form.jsp&ent_id=PMD&tas_id=&auth='+ auth +'&frm_id=FRM003842&key='+key, '',900, 590,'_pmd_plan_modify_'+key); 
};


