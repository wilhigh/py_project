ChkRcat = function(rcat_cd) {

hideButton('CTL11768');

	if(rcat_cd =='SRMRCAT02') {
		showButton('CTL11768'); //장애이관
		hideButton('CTL10495'); //접수
	}else{
		showButton('CTL10495'); //접수
		hideButton('CTL11768'); //장애이관
	}
}
ChkRcat();




