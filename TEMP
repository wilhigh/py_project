20210604

--프로젝트별 룰종류 위반 카운트 
select project_name, count(vio_cnt), vio_type
from 
	(select
		substring(com.kee from '(ssg_.*):.*') project_name,
		count(rul.rule_type) vio_cnt, 
			case 
	       when rul.rule_type in (0, 1)  then 'Code Smell' 
	       when rul.rule_type = 2 then 'Bug' 
	       when rul.rule_type = 3 then 'Vulnerability' 
	              end vio_type
	from issues iss, projects com, rules rul
	where 
	iss.component_uuid = com.uuid
	and iss.rule_id = rul.id
	and com.kee like '%:%'
	and iss.status = 'OPEN'
	group by com.kee, rul.rule_type
	) a
group by project_name, vio_type
order by 1;





