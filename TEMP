20210413


카운트 쿼리 

select a.project_uuid, a.severity, count(rule_id), a.message, b.name
from issues a, rules b 
where a.rule_id = b.id 
and a.status = 'OPEN'
and plugin_name = 'pmd'
and a.project_uuid = 'AXi0V2h7UzDDo89PIHHI'
group by a.project_uuid, a.severity, a.rule_id, message, b.name
--and message = 'Avoid throwing raw exception types.'
;


