서비스지표 종합현황(요약)
서비스지표 팀별 종합현황

select /*+ index(t IX_ESO_CM06) */
          t.cm_id
         ,level lev
         ,(select cod_name from ecf_code where cod_id = cm_cat_cd) as cat_name
         ,(select mtn_name from ecf_mtn where mtn_id = t.cm_mtn_id) as mtn_name
         ,'보기' topo
         ,decode(cnt, null, 'X', 'O') rel_yn
         ,cm_app_own

         ,decode(cm_cpu_disp_yn, '1', 'O', 'X') disp_yn
         ,decode(cm_used, '1', 'O', 'X') use_yn
         ,(select cod_name from ecf_code where cod_id = cm_svr_gb_detail) cm_svr_gb_nm
         ,cm_svr_gb_detail
         ,case when t2.cnt is not null then 'b' else '' end as text_color
         ,cm_tas_id
         ,'수정' as cm_mod
         ,case when (select count(1) from ecf_user_role where uro_rol_id in ('R010','R200')  and  uro_usr_id =  'admin' ) > 0 then 'true' else 'false' end as auth


/*엑셀다운로드 영역*/
      ,(select cod_name from ecf_code where cod_id = cm_cat_cd) CM_CAT_CD_NAME--시스템운영파트
      ,t.CM_NAME --CI명
      ,(select cod_name from ecf_code where cod_id = CM_SVR_GB_DETAIL) CM_SVR_GB_DETAIL_NAME --장비구분
      ,(select cod_name from ecf_code where cod_id = CM_CLA_CD) CM_CLA_CD_NAME --자산구분
      ,CM_APP_TEAM_OLD --(구)APP운영팀
      ,(select dpt_name from ecf_dept where dpt_id = CM_USE_TEAMNAME) CM_USE_TEAMNAME --APP운영팀
      ,(select cod_name from ecf_code where cod_id = CM_STA_CD) CM_STA_CD_NAME --상태
      ,wfe_emp_name --DC 담당자 relation
      ,CM_APP_WORKER_OLD --(구) APP 담당자
      ,app_emp_name --APP담당자 relation
      ,app_emp_id --APP담당자 relation
      ,decode(CM_10_CD,'0', '아니오', '1', '예', '') CM_10_CD --10대 시스템
      ,CM_MED_CD --서버유형
      ,(select cod_name from ecf_code where cod_id = CM_MED_CD) CM_MED_CD_NAME--서버유형
      ,(select cod_name from ecf_code where cod_id = CM_PLACE) CM_PLACE_NAME --센터구분
      ,CM_ETC_LOC --기타위치
      ,CM_RACK_SER_NUM --렉일렬번호
      ,CM_FLOOR --층
      ,CM_RACK_ROW --행
      ,CN_RACK_COL --열
      ,CM_RACK_LOC --랙내위치
      ,CM_SVR_SIZE --크기(U)
      ,CM_RACK_NUM --렉크기(U)
      ,CM_HOSTNAME --호스트명
      ,CM_IPADDR --대표ip
      ,CM_SVR_NAS_IP --기타ip relation
      ,(select cod_name from ecf_code where cod_id = CM_OS_GB) CM_OS_GB_NAME --OS구분
      ,(select cod_name from ecf_code where cod_id = CM_OS_VER) CM_OS_VER_NAME --OS버전1
      ,CM_OS_VER2 --OS버전2
      ,(select cod_name from ecf_code where cod_id = CM_DB_GB) CM_DB_GB_NAME --DB구분
      ,(select cod_name from ecf_code where cod_id = CM_DB_VER) CM_DB_VER_NAME --DB버전1
      ,CM_DB_VER2 --DB버전2
      ,CM_IOS_VER --IOS버전
      ,(select cod_name from ecf_code where cod_id = CM_BACKUP_METHOD) CM_BACKUP_METHOD_NAME --백업방법
      ,(select cod_name from ecf_code where cod_id = CM_SVR_POLESTAR) CM_SVR_POLESTAR_NAME --가상화방법
      ,(select cod_name from ecf_code where cod_id = CM_VIRTURE_YN) CM_VIRTURE_YN_NAME --------------------------------
      ,(select cod_name from ecf_code where cod_id = CM_SYS_GB) CM_SYS_GB_NAME --시스템용도
      ,(select cod_name from ecf_code where cod_id = CM_NW_GB) CM_NW_GB_NAME --N/W구분
      ,(select cod_name from ecf_code where cod_id = CM_MAKE) CM_MAKE --제조사
      ,CM_PRODUCT_NAME --제품모델
      ,CM_SER_NUM --시리얼번호=
      ,substr(CM_INSTALL_DTTM, 0, 8) CM_INSTALL_DTTM --도입일자
      ,substr(CM_TAKOUT_DTTM, 0, 8) CM_TAKOUT_DTTM --반출일
      ,(select cod_name from ecf_code where cod_id = CM_CLUSTER) CM_CLUSTER_NAME --서버클러스터종류
      ,CM_PHY_CNT --물리적수량(서버)
      ,CM_LOG_CNT --논리적수량(서버)
      ,CM_CPU_NUM --CPU개수(물리적)
      ,CM_CORE_NUM --Core개수(1CPU)
      ,CM_CPU_CLOCK --CPU속도(GHz)
      ,CM_MEMORY --메모리(GB)
      ,CM_SVR_INT_DISK --내장DISK
      ,CM_SVR_EXT_DISK --외장DISK
      ,CM_SVR_INT_DISK_GB --DISK사이즈(GB) 내장
      ,CM_SVR_EXT_DISK_GB --DISK사이즈(GB) 외장
      ,CM_STO_DISK_CAPA -- 디스크 (Usable GB)
      ,CM_KW --전기사용량(KW)
      ,CM_INSTALL_PRICE --도입가(천원)
      ,decode(CM_PAY_YN,'0', '아니오', '1', '예', '') CM_PAY_YN --용역료청구대상
      ,CM_PAY_CPU --용역료청구CPU
      ,CM_MTN_ORG --유지보수업체
      ,substr(CM_HW_WARRANTY, 0, 8) CM_HW_WARRANTY --H/W보증기간
      ,decode(CM_SVR_CUBEONE,'0', '아니오', '1', '예', '') CM_SVR_CUBEONE --DB암호화(Cubeone) 설치여부
      ,substr(CM_SVR_CUBEONE_DTTM, 0, 8) CM_SVR_CUBEONE_DTTM --DB암호화(Cubeone) 설치날짜
      ,decode(CM_SVR_DBSAFER,'0', '아니오', '1', '예', '') CM_SVR_DBSAFER --DB접근통제(DBsafer) 설치여부
      ,substr(CM_SVR_DBSAFER_DTTM, 0, 8) CM_SVR_DBSAFER_DTTM --DB접근통제(DBsafer) 설치날짜
      ,decode(CM_SVR_NODESAFER,'0', '아니오', '1', '예', '') CM_SVR_NODESAFER --DB접근통제(Nodesafer) 설치여부
      ,substr(CM_SVR_NODESAFER_DTTM, 0, 8) CM_SVR_NODESAFER_DTTM --DB접근통제(Nodesafer) 설치날짜
      ,decode(CM_SVR_REDCASTLE,'0', '아니오', '1', '예', '') CM_SVR_REDCASTLE --서버접근통제(Redcastle) 설치여부
      ,substr(CM_SVR_REDCASTLE_DTTM, 0, 8) CM_SVR_REDCASTLE_DTTM --서버접근통제(Redcastle) 설치날짜
      ,decode(CM_SVR_LOG,'0', '아니오', '1', '예', '') CM_SVR_LOG --Log변조방지
      ,decode(CM_SVR_WEBFIREWALL,'0', '아니오', '1', '예', '') CM_SVR_WEBFIREWALL --웹방화벽적용
      ,CM_NAT_FIREWALL --NAT방화벽 IP
      ,CM_VAN_FIREWARE --VAN방화벽 IP
      ,CM_EMPTY_FIREWALL --공항방화벽 IP
      ,(select cod_name from ecf_code where cod_id = CM_IP_GB) CM_IP_GB_NAME ----IP구분(위치)
      ,substr(CM_ACTSTART_DTTM, 0, 8) CM_ACTSTART_DTTM --서비스시작일
      ,substr(CM_ACTFINISH_DTTM, 0, 8) CM_ACTFINISH_DTTM --서비스종료일
      ,CM_BIGO --비고
      ,CM_VIRTUAL_IP --VIP
      ,CM_DISK --디스크 구성 정보
         
    from eso_cm t,(
                    select cm_rel_id, count(1) cnt
                      from (
                             select cmr_src_cm_id cm_rel_id 
                               from eso_cm_rel
                            union 
                             select cmr_tgt_cm_id cm_rel_id 
                               from eso_cm_rel
                           ) 
                     group by cm_rel_id 
                   ) t2
                   
                   ,
                  (
                    select 
                           a.wfe_src_id, listagg(b.emp_name,',') within group (order by b.emp_id) wfe_emp_name
                     from eso_wf_emps a, ecf_employee b 
                    where a.wfe_emp_id = b.emp_id 
            and wfe_type_cd = 'CM_ADMIN'
                    group by a.wfe_src_id
                   ) t3,
                  (
                    select 
                           a.wfe_src_id as app_src_id, listagg(b.emp_name,',') within group (order by b.emp_id) app_emp_name
                     from eso_wf_emps a, ecf_employee b 
                    where a.wfe_emp_id = b.emp_id 
            and wfe_type_cd = 'cm_app_ass'
                    group by a.wfe_src_id
                   ) t4,
                  (
                    select 
                           a.wfe_src_id as app_src_id, listagg(b.emp_id,',') within group (order by b.emp_id) app_emp_id
                     from eso_wf_emps a, ecf_employee b 
                    where a.wfe_emp_id = b.emp_id 
            and wfe_type_cd = 'cm_app_ass'
                    group by a.wfe_src_id
                   ) t5
                   
   where t.cm_id = t2.cm_rel_id(+)
     and t.cm_id = t3.wfe_src_id(+)
   and t.cm_id = t4.app_src_id(+)
   and t.cm_id = t5.app_src_id(+)
    and  cm_cat_cd in ( select cod_id from ecf_code where cod_cty_id = 'CMCAT' start with cod_id =  'CAT1300000'  connect by prior cod_id = cod_pid )  and  (cm_mtn_id in (select mto_mtn_id from ecf_mto where mto_org_id =  'IC0') or cm_mtn_id = case when (select count(1) from ecf_user_role where uro_rol_id = 'R010' and uro_usr_id =  null ) > 0 then cm_mtn_id else '' end  or  cm_mtn_id = case when 'IC0' =  'IC0' then cm_mtn_id else '' end or (cm_mtn_id is null and 'IC0' =  'IC0' ) ) 
  start with cm_pid is null
  connect by prior cm_id = cm_pid
  order siblings by t.cm_name, t.cm_id, t.cm_order


