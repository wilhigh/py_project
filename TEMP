ChkCatCd = function (cat_cd) {
  //hideFormGroup('GRP003');
  
  cat_cd +=

	if(cat_cd.substring(0,3) =='SSG' ){ //업무시스템
		chgCondTab('GRP002',cat_cd.substring(0,3));
		showFormGroup('GRP002'); //제원정보
		showFormField('F1009');  //공통구분
		showFormField('F816');   //소유회사
		showFormField('F880');   //없음
		showFormField('F1965');  //매핑관계사	

		hideFormField('F807');   //서버유형
		hideFormField('F881');   //장비구분
		hideFormField('F1295');  //센터구분
		hideFormField('F034');   //반입일
		hideFormField('F035');   //반출일
		hideFormField('F817');   //APP운영팀
		hideFormField('F852');   //APP담당자
		hideFormGroup('GRP003'); //서버정보
		hideFormField('F2133');  //자산관리번호
		
	}else if(cat_cd.substring(0,5)=='CAT01' || cat_cd.substring(0,5)=='CAT02' || cat_cd.substring(0,5)=='CAT07'){ //서버
		chgCondTab('GRP002',cat_cd);
	
		showFormField('F881');   //장비구분
		showFormField('F1295');  //센터구분
		showFormField('F034');   //반입일
		showFormField('F035');   //반출일
		showFormField('F817');   //APP운영팀
		showFormField('F852');   //APP담당자
		showFormField('F2133');  //자산관리번호
		
		showFormGroup('GRP003'); //서버정보
		hideFormField('F1009');  //공통구분
		hideFormField('F816');   //소유회사
		hideFormField('F880');   //없음
		hideFormField('F1965');  //매핑관계사
		
	}else if(cat_cd.substring(0,5)=='CAT13'){
		chgCondTab('GRP002',cat_cd);
		hideFormField('F017');  //상태
		hideFormField('F1701');  //담당고객사
		hideFormField('F1225');  //관리담당자
		hideFormField('F1965');  //매핑관계사
		hideFormField('F1009');  //서버유형
		hideFormField('F807');  //서버유형
		hideFormField('F2133');  //자산관리번호
		hideFormField('F033');  //자산구분
		hideFormField('F780');  //순서
	
		hideFormGroup('GRP003'); //서버정보
		
	}else{
		chgCondTab('GRP002',cat_cd);
		
		showFormGroup('GRP002'); //제원정보
		showFormField('F1009');  //공통구분
		showFormField('F816');   //소유회사
		showFormField('F880');   //없음
		
		hideFormField('F1965');  //매핑관계사
		hideFormField('F881');   //장비구분
		hideFormField('F1295');  //센터구분
		hideFormField('F034');   //반입일
		hideFormField('F035');   //반출일
		hideFormField('F817');   //APP운영팀
		hideFormField('F852');   //APP담당자
		hideFormGroup('GRP003'); //서버정보
		if(cat_cd.substring(0,5)=='CAT03' || cat_cd.substring(0,5)=='CAT04'){ //보안, N/W
			showFormField('F2133');  //자산관리번호
		} else {
			hideFormField('F2133');  //자산관리번호
		}
	}
}

