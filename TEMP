//제목 40자 제한 (결재요청)
controlCTL02131 = function() {
	
	var req_title = $jq('#Field_F005').val(); // 장애제목
	
	if(req_title.length > 40 ){
	    var msg = getText('장애제목은 40자이내로 작성해주십시요');
	    alert(msg);
		$jq('#Field_F005').focus(); 
		return false; 
	}else{
	    return true;
	}
}


/* 장애시간 정보 */
icmDttmFn = function(val,val02){
var tm01 = $jq('#Field_F010_tm').val();
var tm02 = $jq('#Field_F016_tm').val();
var dttm01 = $jq('#Field_F010_dt').val() + ($jq('#Field_F010_tm').val().replace(/:/ig,'')); //처리시작일시
var dttm02 = $jq('#Field_F016_dt').val() + ($jq('#Field_F016_tm').val().replace(/:/ig,'')); //처리완료일시  
    if(tm01 != '' && tm02 != '' ){
  
        var json = getBizData('Get.MinDttm','Dttm01='+dttm01+'&Dttm02='+dttm02);
        var icmM = json.durmin;
        $jq('#Field_F041').val(icmM)
        
    }
};

