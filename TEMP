pmd_reg = function() { 
    showWin('/xefc/jsp/ui/popup_layout.jsp','uri=/xefc/jsp/ui/form/form.jsp&ent_id=PMD&tas_id=&frm_id=FRM003835&act=1&auth=1&_title=', '',600, 590,'_pmd_plan_new'); 
