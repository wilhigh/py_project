   projectKey = "NAMIN" 
   insightKey = "SINCAS-147"
   break;
   case "IC0M42A025": // 재무시스템팀
   projectKey = "NSSHF"     
   insightKey = "SINCAS-4442"
   break;    
   case "IC0M42A450": // 정보서비스사업팀
   projectKey = "BSIS"     
   insightKey = "SINCAS-145"
   break;
   case "IC0M424D02": // 클라우드POS팀
   projectKey = "NAMCP"    
   insightKey = "SINCAS-149"
   break;
   case "IC0M424410": // 신세계푸드
   projectKey = "NAMFD"   
   insightKey = "SINCAS-142"
   break;
   case "IC0M42M771": // 프라퍼티팀
   projectKey = "NAMPP"   
   insightKey = "SINCAS-153"
   break;
   case "IC0M424C10": // 데이터센터팀
   projectKey = "NIMDC"     
   insightKey = "SINCAS-135"
   break;
   case "IC0M42B040": // 정보보안팀
   projectKey = "NISIS"            
   insightKey = "SINCAS-148"
   break;   
}



