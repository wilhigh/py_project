20210706

select t1.ctr_id, t1.ctr_name as ctrname, t1.ctr_term,
       substr(t2.trm_start_dttm,0,4) year,
       t2.trm_name as trmname, 
       get_codename(t2.trm_cat_cd) as trm_cat_name,
       t1.ctr_dpt_id,
       get_dptname(t1.ctr_dpt_id) as ctr_dpt_name,
       to_char(to_date(t2.trm_start_dttm, 'yyyymm'), 'yyyy-mm') as ctr_sdt,
       to_char(to_date(t2.trm_finish_dttm, 'yyyymm'), 'yyyy-mm') as ctr_edt,
       decode(t1.ctr_used, '1', 'Y', 'N') as used,
       (select cod_name from ecf_code where cod_id = t1.ctr_typ_cd) type_name,
       t1.ctr_typ_cd, t1.ctr_consumer, t1.ctr_order 
       ,trunc(( select score from (select nvl(h2_score_cvrt,h1_score_cvrt) as score from edm_total_sla t where t.ctr_id = t1.ctr_id and t.typ_cd = 'CTR' order by mon desc) where rownum = 1),1 )as score
  from ecf_contract t1, ecf_term t2
 where t1.ctr_term = t2.trm_id(+)
  and substr(t2.trm_start_dttm,0,4) =  '2021'  and t1.ctr_term =  'TRM00014'  and (( 'IC0M42A00' =  'IC0M42A00' ) or ('IC0M42A00' <>  'IC0M42A00' and t1.ctr_consumer =  'IC0M42A00') or ( 'admin' =  '203866' ) or ( (select '1' from ecf_user_role where uro_rol_id = 'R120' and uro_usr_id =  '203866' ) = '1' ) )  
 order by ctr_typ_cd,ctr_term desc,ctr_used desc, score desc, ctr_order, ctr_id
 
 


